#
#  This example is a single cpu demonstration of the formation of a simple
#  binary junction using the Fast Multipole Method for calculating remote
#  forces.
#
#  Output generated by this example will be placed under the directory
#  specified by the <dirname> value and includes:
#    restart files
#    povray visualization files
#    various properties files
#
dirname =   "tests/form_binaryjunc_results"  
#
numXcells =   4  
numYcells =   4  
numZcells =   4  
#
numXdoms =   1  
numYdoms =   1  
numZdoms =   1  
#
fmEnabled       = 1
fmMPOrder       = 2
fmTaylorOrder   = 5
fmCorrectionTbl = "inputs/fm-ctab.Ta.600K.0GPa.m2.t5.dat"
#
maxstep =   500
remeshRule =   2  
minSeg =   5.000000e+02  
maxSeg =   2.000000e+03  
rTol =   2.500000e-02  
timestepIntegrator =   "trapezoid"  
#
rc =   1.000000e-01  
MobScrew =   1.000000e+01  
MobEdge =   1.000000e+01  
MobClimb =   1.000000e-04  
mobilityLaw =   "BCC_0"  
#
loadType =   0  
edotdir = [
  0.000000e+00
  0.000000e+00
  1.000000e+00
  ]
appliedStress = [
  0.000000e+00
  0.000000e+00
  0.000000e+00
  0.000000e+00
  0.000000e+00
  0.000000e+00
  ]
savetimers =   0  
savecn =   1  
savecnfreq =   100  
savecncounter =   0  
povray =   1
povrayfreq =   100
povraycounter =   0
saveprop =   1  
savepropfreq =   50  
